@model WebFinalRamm.Models.Cliente

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Detalles de Cliente</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Nit</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nit)
        </dd>
        <dt class="col-sm-2">Razón Social</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RazonSocial)
        </dd>
        <dt class="col-sm-2">Teléfono</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Telefono)
        </dd>
        <dt class="col-sm-2">Usuario de Registro</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UsuarioRegistro)
        </dd>
        <dt class="col-sm-2">Fecha de Registro</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaRegistro)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id"><i class="fa fa-edit"></i> Editar</a> |
    <a asp-action="Index"><i class="fa fa-arrow-alt-circle-left"></i> Volver a la lista</a>
</div>
